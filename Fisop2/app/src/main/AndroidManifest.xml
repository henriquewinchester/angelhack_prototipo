<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.fisopapp.fisop">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Principal"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SolicitarEmprestimo"
            android:label="@string/title_activity_solicitar_emprestimo"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ClienteEmprestimo"
            android:label="@string/title_activity_cliente_emprestimo"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Botoes"
            android:label="@string/title_activity_botoes"></activity>
        <activity
            android:name=".splash"
            android:label="@string/title_activity_splash"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".escolha"
            android:label="@string/title_activity_escolha"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".cadastro"></activity>
    </application>

</manifest>